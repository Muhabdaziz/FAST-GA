        <aspect_ratio>7.74<!--wing aspect ratio--></aspect_ratio>
        <sweep_25 units="deg">0.0<!--sweep angle at 25% chord of wing--></sweep_25>
        <dihedral_angle units="deg">1.0<!--dihedral angle of the wing--></dihedral_angle>
