          <max_deflection units="deg">30.0</max_deflection>
          <span_ratio>0.37</span_ratio>
        </aileron>
        <kink>
          <span_ratio>0.0<!--ratio (Y-position of kink)/(semi-span)--></span_ratio>
        </kink>
        <MAC>
          <at25percent>
            <x units="m">3.51<!--X-position of the 25% of mean aerodynamic chord of wing w.r.t. aircraft nose (drives position of wing along fuselage)--></x>
          </at25percent>
        </MAC>
      </wing>
      <propulsion>
        <engine>
          <count>2.0<!--number of engines--></count>
          <layout>1.0<!--position of engines (1=under the wing / 2=rear fuselage / 3=nose)--></layout>
          <y_ratio>0.311<!--engine position with respect to total span--></y_ratio>
        </engine>
        <tank>
          <LE_chord_percentage>0.08<!--distance between the leading edge and the start of the wing tank as a percentage of the wing chord--></LE_chord_percentage>
          <TE_chord_percentage>0.08<!--distance between the trailing edge device and the start of the wing tank as a percentage of the wing chord--></TE_chord_percentage>
