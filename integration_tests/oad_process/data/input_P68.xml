          </sizing_factor>
        </cs23>
        <main_route>
          <cruise>
            <altitude units="ft">8000.0<!--altitude during cruise phase in sizing mission--></altitude>
          </cruise>
          <descent>
            <descent_rate units="ft/min">-1000.0<!--target descent rate for the aircraft--></descent_rate>
